{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/products",
      "methods": [
        "GET",
        "POST"
      ],
      "dest": "/api/products.js"
    },
    {
      "src": "/api/products/(.*)",
      "methods": [
        "GET",
        "PUT",
        "DELETE"
      ],
      "dest": "/api/products.js"
    }
  ]
}